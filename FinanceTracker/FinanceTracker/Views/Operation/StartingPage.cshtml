@{
    Layout = "_FinanceTrackerLayout";
}
<link href="~/css/operationcss/startingpage.css" rel="stylesheet" />
<link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" rel="stylesheet">
<!--First Section-->
<section class="p-5 p-lg-0 text-center text-sm-start first-section-style">
    <div class="container">
        <div class="d-sm-flex align-items-center justify-content-center">
            <div>
                <h1 class="section1-header-text">Attain Financial Prosperity</h1>
                <h3 class="text-dark mt-4">Track your spending, master your future</h3>
                <p class="fs-4 my-4 text-dark">Embark on a journey to attain
                    the status of a wealthy individual through 
                    diligent effort and strategic financial management</p>
                <button class="btn btn-primary btn-lg">Get Started Now</button>
            </div>
            <img class="img-fluid w-50 d-none d-sm-block" src="~/image/startingpage/savingpig.png" alt=""/>
        </div>
    </div>
</section>
<!--the job of our product-->
<section class="px-5">
    <div class="container px-5">
        <div class="row align-items-center justify-content-center">
            <div class="col-12 col-sm-6">
                <img class="img-fluid w-100" src="~/image/startingpage/maketrising.png" alt="maketrising" />
            </div>
            <div class="col-12 col-sm-6">
                <h1 class="section1-header-text">Track Income and Expenses</h1>
                <p class="fs-4 my-4 text-dark">
                    Record all sources of income and categorize
                    expenses such as groceries, utilities, and
                    entertainment to get a clear overview of your
                    financial inflows and outflows.
                </p>
            </div>
        </div>
    </div>
</section>
<!--the pdf advertise section-->
<section class="px-5">
    <div class="container px-5">
        <div class="row align-items-center justify-content-center">
            <div class="col-12 col-sm-6">
                <h1 class="section1-header-text">Budget Creation and Generate Reports</h1>
                <p class="fs-4 my-4 text-dark">
                    Allow users to set up monthly or yearly budgets based on 
                    their income and expenses. Provide detailed financial reports, including spending summaries,
                    income reports, and balance sheets.
                </p>
            </div>
            <div class="col-12 col-sm-6">
                <img class="img-fluid w-100" src="~/image/startingpage/pdfedit.png" alt="pdfeditor" />
            </div>
        </div>
    </div>
</section>
<!--premium box section-->
<section class="px-5 pb-5">
    <div class="container">
        <div class="text-center mb-3">
            <h1 class="fw-bold text-warning">Premium Plan</h1>
        </div>
        <div class="row d-flex">
            <div class="col-md d-flex mb-2 mb-md-0">
                <div class="card bg-dark text-light d-flex flex-column flex-fill">
                    <div class="card-body">
                        <h1 class="mb-2 text-center">Free</h1>
                        <p class="lead mb-2">For individuals just getting started with finance tracker</p>
                        <ul class="list-unstyled mb-2">
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Assistance with managing cash</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Access to news</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Limited access to Finance wallet</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Limited access to data analysis</li>
                        </ul>
                        <h3>$0 /month</h3>
                    </div>
                    <div class="card-footer my-auto text-center">
                        <button class="btn btn-warning btn-lg w-50">Start now</button>
                    </div>
                </div>
            </div>
            <div class="col-md d-flex mb-2 mb-md-0">
                <div class="card bg-dark text-light d-flex flex-column flex-fill">
                    <div class="card-body">
                        <h1 class="mb-2 text-center">Plus</h1>
                        <p class="lead mb-2">For individuals to amplify with finance tracker</p>
                        <ul class="list-unstyled mb-2">
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Early access to new features</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Access to news</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>No limit access to Finance wallet</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Limited access to data analysis</li>
                        </ul>
                        <h3>$20 /month</h3>
                    </div>
                    <div class="card-footer my-auto text-center">
                        <button class="btn btn-warning btn-lg w-50">Start now</button>
                    </div>
                </div>
            </div>
            <div class="col-md d-flex mb-2 mb-md-0">
                <div class="card bg-dark text-light d-flex flex-column flex-fill">
                    <div class="card-body">
                        <h1 class="mb-2 text-center">Premium</h1>
                        <p class="lead mb-2">For global companies to super manage cash flow</p>
                        <ul class="list-unstyled mb-2">
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Hotline Service</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Everything included in Plus</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Create and Save as pdf file and image</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>Admin console for team member</li>
                            <li class="mb-2"><i class="bi bi-check-lg"></i>No limited access to data analysis</li>
                        </ul>
                        <h3>$30 /month</h3>
                    </div>
                    <div class="card-footer my-auto text-center">
                        <button class="btn btn-warning btn-lg w-50">Start now</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
